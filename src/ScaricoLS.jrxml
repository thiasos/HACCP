	<queryString language="SQL">
		<![CDATA[SELECT
     fornitori.`NOME` AS fornitoriNome,
     articoli.`DESCRIZIONE` AS articoliDescrizione,
     articolimagazzino.`CONFORMITA` AS articolimagazzinoConformita,
     articolimagazzino.`DATASCADENZA` AS articolimagazzinoDatascadenza,
     articolimagazzino.`LOTTO` AS articolimagazzinoLotto,
     articolimagazzino.`TEMPERATURA` AS articolimagazzinoTemperatura,
     articolimagazzino.`TRASPORTO` AS articolimagazzinoTrasporto,
     articolimagazzino.`VALIDITA` AS articolimagazzinoValidita,
     registri.`DATA` AS registriData
FROM
     `fornitori` fornitori INNER JOIN `articoli` articoli ON fornitori.`ID` = articoli.`IDFORNITORE`
     INNER JOIN `articolimagazzino` articolimagazzino ON articoli.`ID` = articolimagazzino.`ARTICOLO_ID`
     INNER JOIN `registri` registri ON articolimagazzino.`REGISTROSCARICO_ID` = registri.`ID` order by registri.DATA]]>
	</queryString>
